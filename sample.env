# Telegram API credentials (get API_ID and API_HASH from https://my.telegram.org/apps)
API_ID=your_api_id
API_HASH=your_api_hash

# Bot token (get from @BotFather on Telegram)
BOT_TOKEN=your_bot_token

# Channel configuration (use @username or channel ID like -1001234567890)
SOURCE_CHANNEL=source_channel_username
TARGET_CHANNEL=target_channel_username

# File settings
FORWARDED_MESSAGES_FILE=forwarded_messages.json
